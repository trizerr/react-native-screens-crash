{
  "name": "clockout",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "react-native-clean-project",
    "android": "rnstl && npx react-native run-android",
    "android-release": "react-native run-android --variant release",
    "gradle-build": "cd android && ./gradlew bundleRelease",
    "ios": "rnstl && react-native run-ios --simulator=\"iPhone 11\"",
    "start": "npx react-native start",
    "test": "jest",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/clockout/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "detox-build-ios": "npx detox build --configuration ios",
    "detox-test-ios-e2e": "yarn run e2e --configuration ios --cleanup",
    "detox-test-ios-integration": "yarn run integration --configuration ios --cleanup",
    "detox-build-android": "npx detox build --configuration android",
    "detox-test-android-e2e": "yarn run e2e --configuration android --cleanup",
    "detox-test-android-integration": "yarn run e2e --configuration android --cleanup",
    "integration": "detox test detox/integration/",
    "e2e": "detox test detox/e2e/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js",
    "pods-update": "yarn install && cd ios && rm -rf ios/Podfile.lock && rm -rf ios/Pods && pod install && cd ..",
    "storybook": "rnstl && start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "reset-cache": "yarn start -- --reset-cache",
    "debug": " REACT_DEBUGGER=\"open -g 'rndebugger://set-debugger-loc?port=8001' ||\" yarn run ios",
    "android-release-build": "cd android && ./gradlew bundleRelease",
    "patch-image-picker": "cp -f src/patches/TOCropView.m ios/Pods/TOCropViewController/Objective-C/TOCropViewController/Views",
    "postinstall": "patch-package && jetify"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.9.6",
    "@invertase/react-native-apple-authentication": "^2.1.2",
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/checkbox": "^0.5.7",
    "@react-native-community/datetimepicker": "^3.4.7",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/push-notification-ios": "^1.10.0",
    "@react-native-firebase/app": "12.9.0",
    "@react-native-firebase/auth": "12.9.0",
    "@react-native-firebase/firestore": "12.9.0",
    "@react-native-firebase/messaging": "12.9.0",
    "@react-native-google-signin/google-signin": "^6.0.0",
    "@react-native-picker/picker": "^1.16.0",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/devtools": "^5.1.22",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@storybook/react": "^6.2.5",
    "@types/styled-components-react-native": "^5.1.1",
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "date-fns": "^2.22.1",
    "firebase": "^8.6.2",
    "jwt-decode": "^3.1.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-android-location-services-dialog-box": "^2.8.2",
    "react-native-code-input": "^1.0.6",
    "react-native-date-picker": "^3.2.10",
    "react-native-dotenv": "^2.5.5",
    "react-native-drawer": "^2.5.1",
    "react-native-fast-image": "^8.5.8",
    "react-native-fbsdk-next": "^4.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-google-places-autocomplete": "berezynskyi/react-native-google-places-autocomplete",
    "react-native-image-crop-picker": "0.36.3",
    "react-native-in-app-review": "^3.2.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-location": "^2.5.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-modal-datetime-picker": "^12.0.0",
    "react-native-modalize": "^2.0.10",
    "react-native-pager-view": "^5.1.7",
    "react-native-permissions": "^3.0.3",
    "react-native-picker-select": "^8.0.4",
    "react-native-push-notification": "^7.3.1",
    "react-native-reanimated": "2.2.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.9.0",
    "react-native-shadow": "^1.2.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-storybook-loader": "^2.0.2",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-tab-view": "^3.0.1",
    "react-native-text-gradient": "^0.1.7",
    "react-native-touchable-scale": "^2.1.2",
    "react-native-user-defaults": "^0.1.5",
    "react-native-uuid": "^2.0.1",
    "react-native-version-check": "^3.4.2",
    "react-redux": "^7.2.2",
    "react-spring": "^9.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "styled-components": "^5.2.3",
    "use-debounce": "^6.0.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@bugsnag/source-maps": "^2.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-theming/storybook-addon": "^1.1.1",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "5.3.21",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-backgrounds": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-ondevice-notes": "^5.3.23",
    "@storybook/addon-options": "5.3.21",
    "@storybook/addons": "5.3.21",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^18.22.0",
    "eslint": "^7.23.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-styled-components-a11y": "^0.0.34",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-circus": "26",
    "jetifier": "^2.0.0",
    "lint-staged": "^10.5.4",
    "metro-config": "^0.65.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "prop-types": "^15.7.2",
    "react-dom": "17.0.1",
    "react-native-clean-project": "^3.6.3",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "eslint --max-warnings=0"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
